		<Language Name="Malay" Id="ms"/>
		<Language Name="Telugu" Id="te"/>
		<Language Name="Kinyarwanda" Id="ro"/>
		<Language Name="Hungarian" Id="hu"/>
	</Languages>
	<FeatureAllocation>
		<!--
		In the new world order, there are only these Features in the FW installer:
			FW_Core
				FlexMovies
				LOC
					Various localized languages
		-->
		<!-- Allocation of files in Output\Release is generally done by analyzing FieldWorks.targets -->
		<!-- and comparing the resulting file sets. Here we define heuristics for dealing with files in -->
		<!-- DistFiles. Each main element defines what section of the installer is being addressed, and -->
		<!-- then follows a selection from the following 4 types: -->
		<!-- <File PathContains="*partial path appearing anywhere in a DistFiles file that is needed in the current section*"/> -->
		<!-- <File PathEnds="*partial path appearing at the end of a DistFiles file that is needed in the current section*"/> -->
		<!-- <Except PathContains="*partial path appearing anywhere in a DistFiles file that must not appear in the current section*"/> -->
		<!-- <Except PathEnds="*partial path appearing at the end of a DistFiles file that must not appear in the current section*"/> -->
		<!-- Be aware that the Except elements take precedence over the File elements. -->
		<FlexMoviesOnly>
			<!-- Define heuristics for FLEx movie files: -->
			<File PathContains="Language Explorer\Movies"/>
		</FlexMoviesOnly>
		<Localization>
			<!-- Define heuristics for (non-TE) localization files, where the {0} parameter is replaced by the language code: -->
			<File PathContains="\{0}\"/>
			<File PathEnds="\strings-{0}.xml"/>
		</Localization>
	</FeatureAllocation>
	<ExtraFiles>
		<!-- Define files needed in the installer that are not built locally or part of source control: -->
		<!-- Format: <File URL="*URL of an extra file*" Destination="*Local relative path to download file to*"/> -->
		<File URL="http://downloads.palaso.org/FlexBridge/Alpha/FLExBridgeInstaller.msi" Destination="DistFiles\Installers\FLExBridgeInstaller.msi"/>
		<File URL="http://build.palaso.org/guestAuth/repository/download/bt333/.lastSuccessful/ChorusHubInstaller.msi" Destination="DistFiles\Installers\ChorusHubInstaller.msi"/>
	</ExtraFiles>
