<InstallerConfig>
	<Languages>
		<!-- Define localization languages: -->
		<!-- Format: <Language Name="*language name*" Id="*folder in output\release where localized DLLs get built*"/> -->
		<Language Name="Spanish" Id="es"/>
		<Language Name="French" Id="fr"/>
		<Language Name="Indonesian" Id="id"/>
		<Language Name="Portuguese" Id="pt"/>
		<Language Name="Russian" Id="ru"/>
